{
  "name": "circleci-status",
  "displayName": "circleci-status",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:.circleci/config.yml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [
      {
        "title": "CircleCI Status configuration",
        "properties": {
          "circleciStatus.apiToken": {
            "type": "string",
            "markdownDescription": "CircleCI API token. See the [CircleCI docs](https://circleci.com/docs/api/#add-an-api-token) for getting API token."
          },
          "circleciStatus.projectName": {
            "type": "string",
            "markdownDescription": "CircleCI project name. Defaults to your workspace folder name."
          }
        }
      }
    ],
    "commands": [
      {
        "command": "myExtension.sayHello",
        "title": "Say Hello"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "myExtension.sayHello"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.38.0",
    "axios": "^0.19.0",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0"
  }
}
